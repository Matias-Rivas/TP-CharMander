#ifndef HEADERS_STRUCTS_H_
#define HEADERS_STRUCTS_H_

typedef struct
{
	int tiempoChequeoDeadlock;
	int modoBatalla;
	char* algoritmo;
	int quantum;
	int retardo;
	char* ip;
	char* puerto;
}MetadataMapa;

typedef struct
{
	char* nombreMapa;
	char* dirPokedex;

}ParametrosMapa;


#endif /* HEADERS_STRUCTS_H_ */
