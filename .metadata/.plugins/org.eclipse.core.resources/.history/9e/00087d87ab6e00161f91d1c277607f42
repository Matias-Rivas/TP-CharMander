#include <stdio.h>

typedef struct
{


}ParametrosMapa;

void verificarParametros(int argc)
{
	if(argc!=3)
	{
		printf("Error - Faltan parametros");
		exit(1);
	}
}

int main(int argc, char** argv)
{
	ParametrosMapa parametros;

	verificarParametros(argc); //Verificamos que la cantidad de Parametros sea correcta

}
